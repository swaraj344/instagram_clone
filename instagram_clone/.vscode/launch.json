{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "instagram_clone_prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "production",
                "--dart-define",
                "ENVIRONMENT=PRODUCTION",
                "-t",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "instagram_clone_dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "ENVIRONMENT=DEV",
                "-t",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "instagram_clone (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "instagram_clone_prod (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "production",
                "--dart-define",
                "ENVIRONMENT=PRODUCTION",
                "-t",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "instagram_clone_dev (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "ENVIRONMENT=DEV",
                "-t",
                "lib/main_dev.dart"
            ]
        }
    ]
}